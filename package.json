{
  "author": {
    "email": "paul@inc.sh",
    "name": "Paul Anthony Webb"
  },
  "dependencies": {
    "choo": "^6.13.0"
  },
  "description": "A super simple and lightweight starting point for your Electron-based app.",
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-stage-2": "^7.0.0-rc.1",
    "choo-devtools": "^2.5.1",
    "cross-env": "^5.2.0",
    "electron": "~2.0.7",
    "electron-packager": "^12.1.1",
    "electron-reloader": "^0.2.0",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.64.1",
    "rollup-plugin-babel": "beta",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-uglify": "^4.0.0",
    "sass": "^1.13.0",
    "snazzy": "^7.1.1",
    "standardx": "^2.1.0",
    "updates": "^4.1.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "main": "dist/main.js",
  "name": "electron-starter",
  "packageName": "Electron Starter",
  "private": true,
  "scripts": {
    "build": "rollup --config && npm run css",
    "css": "sass --update src/app/sass:dist --style compressed",
    "package": "npm run build && electron-packager .",
    "start": "npm run build && run-s start:*",
    "start:electron": "electron .",
    "test": "run-s test:*",
    "test:dependencies": "updates -u ./",
    "test:lint": "standardx --verbose | snazzy",
    "watch": "run-p watch:*",
    "watch:development": "rollup --config --watch",
    "watch:electron": "electron .",
    "watch:sass": "sass --watch src/app/sass:dist --style compressed"
  },
  "standardx": {
    "ignore": [
      "dist"
    ]
  },
  "version": "0.0.0"
}
